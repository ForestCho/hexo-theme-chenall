<%if (page.prev || page.next) {%>
<ul id="pagination" class="pagination pagination-lg">
  <%- paginator({prev_text:'&laquo;',next_text:'&raquo;'}).replace(/<a.+?\/a>/g,'<li>$&</li>').replace(/<span[^>]+?current.+?\/span>/g,'<li class="active">$&</li>').replace(/<span.+?\/span>/g,'<li>$&</li>')%>
  <div class="clearfix"></div>
</ul>
<%}%>
